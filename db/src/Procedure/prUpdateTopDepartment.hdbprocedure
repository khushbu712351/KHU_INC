PROCEDURE "prUpdateTopDepartment"(
    IN USRID NVARCHAR(40),      -- User ID
    TODPT INTEGER               --Top Department
    )
   LANGUAGE SQLSCRIPT 
   SQL SECURITY INVOKER
   --READS SQL DATA 
   AS
BEGIN
   UPDATE "ROWLO"."MDT_T_EMPDT" SET TODPT = :TODPT,CHNBY = SESSION_CONTEXT('APPLICATIONUSER'), CHNDT = CURRENT_DATE,CHNTM = CURRENT_TIME 
   WHERE USRID = :USRID AND ISDEL = '0';
   
END